    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "pdfjs-dist": "^2.9.359",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.8"
  },
  "scripts": {
